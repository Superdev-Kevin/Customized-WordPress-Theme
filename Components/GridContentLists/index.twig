<div class="flyntComponent" is="flynt-grid-content-lists">
  <div class="container">
    {% if preContentHtml %}
      <div class="preContent">{{ preContentHtml|e('wp_kses_post') }}</div>
    {% endif %}
    <ul class="gridLinkList">
      {% for singleBlock in blocks %}
        <li class="gridLinkList-item">
          <div class="content">
          {{ singleBlock.contentHtml|e('wp_kses_post') }}
          </div>
        </li>
      {% endfor %}
    </ul>
  </div>
</div>
