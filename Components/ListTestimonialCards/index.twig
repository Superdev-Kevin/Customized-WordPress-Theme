<div class="flyntComponent componentSpacing {{ options.theme }}" is="flynt-list-testimonial-cards">
  <div class="container centerMaxWidthContainer">
    {% if preContentHtml %}
      <div class="preContent">
        {{ preContentHtml|e('wp_kses_post') }}
      </div>
    {% endif %}
    <ul class="testimonialsList">
      {% for testimonial in testimonials %}
        <li class="testimonial">
          <div class="content boxPadding boxShadow themeReset">
            {{- testimonial.contentHtml|e('wp_kses_post') -}}
          </div>
        </li>
      {% endfor %}
    </ul>
  </div>
</div>
