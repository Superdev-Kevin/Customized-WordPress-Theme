<div class="flyntComponent" is="flynt-list-testimorials-cards">
  <div class="container">
    {% if preContentHtml %}
      <div class="preContent">{{ preContentHtml|e('wp_kses_post') }}</div>
    {% endif %}
    <ul class="testimorialsList">
      {% for testimorial in testimorialsList %}
        <li class="testimorial">
          <div class="content">
            {{ testimorial.contentHtml|e('wp_kses_post') }}
          </div>
        </li>
      {% endfor %}
    </ul>
  </div>
</div>
