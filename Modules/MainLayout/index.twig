<!DOCTYPE html>
<html class="no-js {{ bodyClass }}" lang="{{ site.language }}" dir="{{ dir }}" is="wps-main-layout">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <link href={{ faviconPath }} rel="shortcut icon" type="image/png">
    <link rel="alternate" type="application/rss+xml" title="{{ feedName }}" href="{{ feedHref }}">
    <link rel="apple-touch-icon" href="{{ appleTouchIcon180x180Path }}">
    {{ wp_head }}
  </head>
  <body>
    {{ area('mainTemplate') }}
    {{ wp_footer }}
  </body>
</html>
